{
    "compilerOptions": {
        "allowJs": true,
        "alwaysStrict": true,
        "allowImportingTsExtensions": true,
        "allowUnreachableCode": false,
        "allowSyntheticDefaultImports": true,
        "checkJs": false,
        "esModuleInterop": true,
        "lib": ["DOM", "DOM.Iterable", "ES5"],
        "noEmit": true,
        "noImplicitAny": true,
        "outDir": "/dist/",
        "strict": true,
    }
}